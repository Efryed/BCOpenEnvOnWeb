{
  "name": "fjre-alsimpletools",
  "displayName": "FJRE AL Simple Tools",
  "description": "A set of simple tools for AL development in Business Central.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.107.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "fjre-alsimpletools_toolsContainer",
          "title": "FJRE AL Simple Tools",
          "icon": "./src/images/icon.svg"
        }
      ]
    },
    "views": {
      "efryedal_simpletools_toolsContainer": [
        {
          "id": "fjre-alsimpletools-EnvViewContainer",
          "name": "FJRE AL Environments",
          "icon": "./src/images/icon.svg",
          "contextualTitle": "Package Explorer"
        }
      ]
    },
    "commands": [
      {
        "command": "fjre-alsimpletools.openEnvOnWeb",
        "title": "FJRE AL Simple Tools: Open Environment on Web"
      },
      {
        "command": "fjre-alsimpletools.buildALProject",
        "title": "FJRE AL Simple Tools: Build AL Project"
      }
    ]
  },
  "icon": "./src/images/icon.png",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.107.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.48.1",
    "eslint": "^9.39.1",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}